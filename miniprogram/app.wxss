/**app.wxss**/
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 


/* 小图标 */
@font-face {
  font-family: 'iconfont';  /* project id 1041080 */
  src: url('//at.alicdn.com/t/font_1041080_n1vwsplt78t.eot');
  src: url('//at.alicdn.com/t/font_1041080_n1vwsplt78t.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1041080_n1vwsplt78t.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1041080_n1vwsplt78t.woff') format('woff'),
  url('//at.alicdn.com/t/font_1041080_n1vwsplt78t.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1041080_n1vwsplt78t.svg#iconfont') format('svg');
}


.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

